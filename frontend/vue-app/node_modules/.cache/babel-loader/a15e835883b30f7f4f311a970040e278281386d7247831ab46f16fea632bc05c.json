ord\" v-model=\"user.c_password\" class=\"form-control\" placeholder=\"Confirm Password\" required>\r\n                </div>\r\n                <button class=\"btn btn-primary btn-block mt-4\">Register</button>\r\n              </form>\r\n            </div>\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </div>\r\n  </main>\r\n</template>\r\n\r\n<script>\r\nimport axios from 'axios';\r\n/* eslint-disable vue/multi-word-component-names */\r\nexport default {\r\n    name: 'Register',\r\n    data(){\r\n        return{\r\n            user: {},\r\n            first_name: '',\r\n            last_name: '',\r\n            email: '',\r\n            password: '',\r\n            c_password: '',\r\n            errors: []\r\n        }\r\n    },\r\n    methods:{\r\n        async handleSubmit(){\r\n            this.errors = [];\r\n            if(!this.user.first_name){\r\n                  this.errors.push(\"First Name is required\");\r\n            }\r\n            if(!this.user.last_name){\r\n                this.errors.push(\"Last Name is required\");\r\n            }\r\n            if(!this.user.email){\r\n                this.errors.push(\"Email is required\");\r\n            }\r\n            if(!this.user.password){\r\n                this.errors.push(\"Password is required\");\r\n            }\r\n            if(!this.user.c_password){\r\n                this.errors.push(\"Confirm Password is required\");\r\n            }\r\n            if(this.user.c_password != this.user.c_password){\r\n                this.errors.push(\" Password does not match\");\r\n            }\r\n\r\n            if(!this.errors.length){\r\n              let formData = new FormData();\r\n              formData.append('first_name', this.user.first_name);\r\n              formData.append('last_name', this.user.last_name);\r\n              formData.append('email', this.user.email);\r\n              formData.append('password', this.user.password);\r\n\r\n              let url = 'http://127.0