{"ast":null,"code":"import axios from 'axios';\n/* eslint-disable vue/multi-word-component-names */\nexport default {\n  name: 'Register',\n  data() {\n    return {\n      user: {},\n      first_name: '',\n      last_name: '',\n      email: '',\n      password: '',\n      c_password: '',\n      errors: []\n    };\n  },\n  methods: {\n    async handleSubmit() {\n      this.errors = [];\n      if (!this.errors.length) {\n        let formData = new FormData();\n        formData.append('first_name', this.user.first_name);\n        formData.append('last_name', this.user.last_name);\n        formData.append('email', this.user.email);\n        formData.append('password', this.user.password);\n        formData.append('c_password', this.user.c_password);\n        let url = 'http://127.0.0.1:8000/api/register';\n        await axios.post(url, formData).then(response => {\n          console.log(response);\n        }).catch(error => {\n          // Check if the error has a response and a message\n          if (error.response && error.response.data && error.response.data.message) {\n            // Assign the error message to the errors property\n            this.errors = error.response.data.message;\n          } else {\n            // If no error message is available, display a generic error\n            this.errors = 'An error occurred while processing your request.';\n          }\n        });\n      }\n    }\n  }\n};","map":{"version":3,"names":["axios","name","data","user","first_name","last_name","email","password","c_password","errors","methods","handleSubmit","length","formData","FormData","append","url","post","then","response","console","log","catch","error","message"],"sources":["C:\\Users\\bebex\\Desktop\\crud-app\\src\\components\\Register.vue"],"sourcesContent":["<template>\r\n    <main class=\"login-form mt-5\">\r\n    <div class=\"container\">\r\n      <div class=\"row justify-content-center\">\r\n        <div class=\"col-md-6\">\r\n          <div class=\"card\">\r\n            <div class=\"card-header bg-primary text-white\">Sign Up</div>\r\n            <div class=\"card-body\">\r\n              <div class=\"alert alert-danger mt-4\" v-if=\"errors.length\">\r\n                    <ul class=\"mb-0\">\r\n                        <li v-for=\"(error, index) in errors\" :key=\"index\">\r\n                            {{error}}\r\n                        </li>\r\n                    </ul>\r\n                </div>\r\n              <form @submit.prevent=\"handleSubmit\" novalidate>\r\n                <div class=\"form-group\">\r\n                  <label for=\"email_address\">First Name</label>\r\n                  <input type=\"text\" v-model=\"user.first_name\" class=\"form-control\" required autofocus placeholder=\"Enter First Name\">\r\n                </div>\r\n\r\n                <div class=\"form-group\">\r\n                  <label for=\"email_address\">Last Name</label>\r\n                  <input type=\"text\" v-model=\"user.last_name\" class=\"form-control\" required autofocus placeholder=\"Enter Last Name\">\r\n                </div>\r\n\r\n                <div class=\"form-group\">\r\n                  <label for=\"email_address\">E-Mail Address</label>\r\n                  <input type=\"email\" v-model=\"user.email\" class=\"form-control\" required autofocus placeholder=\"Enter Email\">\r\n                </div>\r\n\r\n                <div class=\"form-group\">\r\n                  <label for=\"password\">Password</label>\r\n                  <input type=\"password\" v-model=\"user.password\" class=\"form-control\" placeholder=\"Enter Password\" required>\r\n                </div>\r\n\r\n                <div class=\"form-group\">\r\n                  <label for=\"password\">Confirm Password</label>\r\n                  <input type=\"password\" v-model=\"user.c_password\" class=\"form-control\" placeholder=\"Confirm Password\" required>\r\n                </div>\r\n                <button class=\"btn btn-primary btn-block mt-4\">Register</button>\r\n              </form>\r\n            </div>\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </div>\r\n  </main>\r\n</template>\r\n\r\n<script>\r\nimport axios from 'axios';\r\n/* eslint-disable vue/multi-word-component-names */\r\nexport default {\r\n    name: 'Register',\r\n    data(){\r\n        return{\r\n            user: {},\r\n            first_name: '',\r\n            last_name: '',\r\n            email: '',\r\n            password: '',\r\n            c_password: '',\r\n            errors: []\r\n        }\r\n    },\r\n    methods:{\r\n        async handleSubmit(){\r\n            this.errors = [];\r\n            if(!this.errors.length){\r\n              let formData = new FormData();\r\n              formData.append('first_name', this.user.first_name);\r\n              formData.append('last_name', this.user.last_name);\r\n              formData.append('email', this.user.email);\r\n              formData.append('password', this.user.password);\r\n              formData.append('c_password', this.user.c_password);\r\n\r\n              let url = 'http://127.0.0.1:8000/api/register';\r\n              await axios.post(url, formData).then((response) => {\r\n                  console.log(response);\r\n              }).catch(error => {\r\n                  // Check if the error has a response and a message\r\n          if (error.response && error.response.data && error.response.data.message) {\r\n              // Assign the error message to the errors property\r\n              this.errors = error.response.data.message;\r\n          } else {\r\n              // If no error message is available, display a generic error\r\n              this.errors = 'An error occurred while processing your request.';\r\n          }\r\n              });\r\n            }\r\n\r\n        }\r\n    }\r\n}\r\n</script>"],"mappings":"AAmDA,OAAOA,KAAI,MAAO,OAAO;AACzB;AACA,eAAe;EACXC,IAAI,EAAE,UAAU;EAChBC,IAAIA,CAAA,EAAE;IACF,OAAM;MACFC,IAAI,EAAE,CAAC,CAAC